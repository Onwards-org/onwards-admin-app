{
  "name": "onwards-admin-app",
  "version": "1.0.0",
  "description": "Admin platform for Onwards community organization",
  "main": "dist/server/index.js",
  "type": "module",
  "scripts": {
    "dev": "node scripts/simple-dev.js",
    "dev:server": "tsx watch --clear-screen=false src/server/index.ts",
    "dev:client": "vite --host",
    "dev:detached": "node scripts/robust-dev.js start",
    "dev:status": "node scripts/robust-dev.js status",
    "dev:logs": "tail -f logs/backend.log logs/frontend.log",
    "dev:stop": "node scripts/robust-dev.js stop",
    "dev:restart": "node scripts/robust-dev.js restart",
    "dev:clean": "node scripts/robust-dev.js stop",
    "dev:health": "node scripts/robust-dev.js status",
    "dev:ports": "bash -c 'echo \"=== Active Ports ===\" && ss -tlnp 2>/dev/null | grep -E \":(3001|8080)\" || echo \"No development ports in use\"'",
    "prod:start": "node scripts/prod-server.js start",
    "prod:stop": "node scripts/prod-server.js stop",
    "prod:restart": "node scripts/prod-server.js restart",
    "prod:status": "node scripts/prod-server.js status",
    "prod:logs": "node scripts/prod-server.js logs",
    "prod:build": "node scripts/prod-server.js build",
    "ssl:setup": "node scripts/ssl-setup.js",
    "pm2:start": "npm run build && pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop onwards-admin",
    "pm2:restart": "npm run build && pm2 restart onwards-admin",
    "pm2:status": "pm2 status",
    "pm2:logs": "pm2 logs onwards-admin",
    "pm2:delete": "pm2 delete onwards-admin",
    "build": "vite build",
    "build:client": "vite build",
    "build:server": "tsc -p tsconfig.server.json --skipLibCheck",
    "build:vercel": "npm run build:client",
    "start": "node dist/server/index.js",
    "typecheck": "vue-tsc --noEmit && tsc --noEmit -p tsconfig.server.json",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@google/maps": "^1.1.3",
    "@googlemaps/js-api-loader": "^1.16.8",
    "@types/bcryptjs": "^2.4.6",
    "@types/multer": "^1.4.13",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.1",
    "pdfkit": "^0.15.0",
    "pg": "^8.11.3",
    "pinia": "^2.1.7",
    "valibot": "^0.23.0",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/google.maps": "^3.58.1",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.9.0",
    "@types/pdfkit": "^0.13.4",
    "@types/pg": "^8.10.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.1.4",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.22"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Onwards-org/onwards-admin-app.git"
  },
  "author": "Lucy Needham",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Onwards-org/onwards-admin-app/issues"
  },
  "homepage": "https://github.com/Onwards-org/onwards-admin-app#readme"
}
