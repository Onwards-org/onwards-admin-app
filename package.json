{
  "name": "onwards-admin-app",
  "version": "1.0.0",
  "description": "Admin platform for Onwards community organization",
  "main": "dist/server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "tsx watch --clear-screen=false src/server/index.ts",
    "dev:client": "vite",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "start": "node dist/server/index.js",
    "typecheck": "vue-tsc --noEmit && tsc --noEmit -p tsconfig.server.json",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix"
  },
  "dependencies": {
    "@google/maps": "^1.1.3",
    "@koa/cors": "^5.0.0",
    "bcrypt": "^5.1.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "pdfkit": "^0.15.0",
    "chart.js": "^4.4.0",
    "valibot": "^0.23.0",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5",
    "pinia": "^2.1.7"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.9.0",
    "@types/pg": "^8.10.7",
    "@types/pdfkit": "^0.13.4",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^4.0.0-alpha.25",
    "tsx": "^4.1.4",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.22"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Onwards-org/onwards-admin-app.git"
  },
  "author": "Lucy Needham",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Onwards-org/onwards-admin-app/issues"
  },
  "homepage": "https://github.com/Onwards-org/onwards-admin-app#readme"
}